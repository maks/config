#!/bin/bash

case $1 in
    p) echo PlayPause > ~/.config/gmusicbrowser/gmusicbrowser.fifo
        ;;
    r) echo PrevSong > ~/.config/gmusicbrowser/gmusicbrowser.fifo
        ;;
    n) echo NextSong > ~/.config/gmusicbrowser/gmusicbrowser.fifo
        ;;
    s) echo DecVolume > ~/.config/gmusicbrowser/gmusicbrowser.fifo
        ;;
    l) echo IncVolume > ~/.config/gmusicbrowser/gmusicbrowser.fifo
        ;;
    *)
        ;;
esac

